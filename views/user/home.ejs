<div class='container'>
  <div class='row'>
    <div class='col-lg-12'>
      Welcome, <%= req.session.userName %>
    </div>
  </div>
  <div class='row'>
    <div class='col-lg-12'>
      <input type='text' class='form-control' placeholder='Search for people here'/>
    </div>
  </div>
  <div class='row'>
    <div class='col-lg-8'>
      <div class='row'>
        <div class='col-lg-12 chat-box form-control'>
        </div>
        <br>
        <div class='col-lg-12 chat-text-box-wrapper'>
          <input type='text' class='chat-text-box form-control' data-recipient-id='2' placeholder='Type messages here'/>
        </div>
      </div>
    </div>
    <div class='col-lg-4 friends-box'>
    </div>
  </div>
</div>
<input type='hidden' name='csrf' class='csrf-token-home' value='<%= _csrf %>'/>
<input type='hidden' class='home-page'/>
